{
  "name": "react-boilerplate-typescript",
  "version": "1.0.0",
  "description": "Boilerplate for react apps using typescript",
  "main": "app/index.tsx",
  "scripts": {
    "start": "webpack-dev-server --hot --history-api-fallback --env development",
    "build": "webpack --env production",
    "format": "prettier --write",
    "lint:js": "eslint",
    "lint:css": "stylelint 'app/**/*.{ts,tsx}'",
    "lint-and-fix:js": "npm run lint:js -- --fix",
    "lint-and-fix:css": "npm run lint:css -- --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": [
      "npm run lint-and-fix:js",
      "npm run lint-and-fix:css",
      "npm run format",
      "git add"
    ]
  },
  "keywords": [],
  "author": "Danilo Trevelin",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "10.0.17",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "react-hot-loader": "4.12.12",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "reselect": "4.0.0",
    "sanitize.css": "11.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@emotion/babel-preset-css-prop": "10.0.17",
    "@hot-loader/react-dom": "16.9.0",
    "@types/enzyme": "3.10.3",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-helmet": "5.0.9",
    "@types/react-redux": "7.1.2",
    "@types/react-router-dom": "4.3.5",
    "@types/redux-logger": "3.0.7",
    "@types/webpack": "4.39.1",
    "@types/webpack-env": "1.14.0",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-emotion": "10.0.14",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "fork-ts-checker-webpack-plugin": "1.5.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "jest-emotion": "10.0.17",
    "lint-staged": "9.2.5",
    "mini-css-extract-plugin": "0.8.0",
    "npm-check-updates": "3.1.21",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "1.18.2",
    "style-loader": "1.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-prettier": "1.1.1",
    "typescript": "3.6.2",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.7",
    "webpack-dev-server": "3.8.0",
    "webpack-merge": "4.2.2"
  }
}
